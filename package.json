{
    "name": "myportfolio",
    "version": "1.0.0",
    "description": "My Personal Portfolio",
    "main": "src/index.ts",
    "scripts": {
        "deploy": "npm run build && git add . && git commit && git push origin",
        "build": "npx tsc",
        "format": "lint && prettier",
        "start-local": "nodemon src/index.ts",
        "start": "node dist/index.js",
        "jasmine": "jasmine",
        "test": "npm run build && npm run jasmine",
        "lint": "eslint . --ext .ts",
        "lintfix": "eslint . --ext .ts --fix",
        "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BR19-gh/myportfolio.git"
    },
    "keywords": [
        "portfolio"
    ],
    "author": "BR19-gh",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/BR19-gh/myportfolio/issues"
    },
    "devDependencies": {
        "@types/ejs": "^3.1.1",
        "@types/express": "^4.17.13",
        "@types/express-rate-limit": "^6.0.0",
        "@types/jasmine": "^4.0.3",
        "@types/node": "^18.0.0",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "eslint": "^8.17.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.2.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "jasmine": "^4.2.1",
        "jasmine-spec-reporter": "^6.0.0",
        "nodemon": "^2.0.18",
        "prettier": "^2.7.0",
        "supertest": "^6.2.3",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "ejs": "^3.1.8",
        "express": "^4.18.1",
        "express-rate-limit": "^6.4.0"
    },
    "homepage": "https://github.com/BR19-gh/myportfolio#readme"
